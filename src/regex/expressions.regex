ID ::=              [a-z] [a-z0-9]*
NAT ::=             [0] | [1-9][0-9]*
UnsignedInt ::=     [0] | ([1-9][0-9]*)
SignedInt ::=       [\+\-]? UnsignedInt
UnsignedReal ::=    UnsignedInt "." [0-9]+ ([eE] SignedInt)?
UnsignedReal ::=    UnsignedInt [eE] SignedInt
FLOAT ::=            [\+\-]? UnsignedReal